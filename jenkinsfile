#!groovy
properties([disableConcurrentBuilds()])

pipeline {
    agent any
    options {
        buildDiscarder(logRotator(numToKeepStr: '10', artifactNumToKeepStr: '10'))
        timestamps()
    }
    stages {
        stage("First step") {
            steps {
                script {                 
  			def remote = [:]
  			remote.name = 'root'
  			remote.host = '165.232.65.123'
  			remote.allowAnyHosts = true
  			withCredentials([usernamePassword(credentialsId: 'SSH-TESTT', usernameVariable: 'root')]) {
   			remote.user = "${username}"
  	}
  			sshCommand remote: remote, command: "pwd"
            }
        }
    }
}
}
